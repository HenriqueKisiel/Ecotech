                            <div class="mb-3 d-flex flex-column">
                                <span class="fs-2 fw-bold">
                                    Editar Pessoa Jurídica
                                </span>
                                <small class="text-muted">
                                    Preencha os dados de cadastro e clique em salvar.
                                </small>
                            </div>
                            <div>
                                <a href="/pessoa" class="btn btn-secondary shadow-lg">Voltar</a>
                            </div>
                        </div>
                        <div class="card shadow-lg">
                            <div class="card-header d-none">
                                <div class="card-title">
                                    <span class="fs-2 fw-bold"></span>
                                </div>
                            </div>
                            <div class="card-body">
                                <form id="formEditarPessoaJuridica" action="/juridicaEditar" method="POST">
                                    <div class="form-group">
                                        <input type="hidden" name="codigo"
                                            value="{{pessoa_juridica.cd_pessoa_juridica}}">
                                    </div>
                                    <div class="row">
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="fw-bold">Nome Fantasia</label>
                                                <input type="text" name="nomeFantasia"
                                                    value="{{pessoa_juridica.nm_fantasia}}" class="form-control"
                                                    placeholder="Insira o nome/razão">
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="fw-bold">Razão Social</label>
                                                <input type="text" name="razaoSocial"
                                                    value="{{pessoa_juridica.nm_razao_social}}" class="form-control"
                                                    placeholder="Insira o nome/razão">
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="fw-bold">E-mail</label>
                                                <input type="email" name="email" value="{{pessoa_juridica.ds_email}}"
                                                    class="form-control" placeholder="Insira o E-mail">
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="fw-bold">CNPJ</label>
                                                <input maxlength="18" type="text" name="cnpj"
                                                    value="{{pessoa_juridica.nr_cnpj}}" class="form-control"
                                                    placeholder="Insira o CNPJ">
                                                <small class="form-text text-muted">Em caso de pessoa jurídica.</small>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="fw-bold">Telefone</label>
                                                <input maxlength="18" type="text" name="telefone"
                                                    value="{{pessoa_juridica.nr_telefone}}" class="form-control"
                                                    placeholder="Nr de telefone">
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="fw-bold">Endereço</label>
                                                <input maxlength="50" type="text" name="endereco"
                                                    value="{{pessoa_juridica.ds_endereco}}" class="form-control"
                                                    placeholder="Preencha o endereço">
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="fw-bold">CEP</label>
                                                <input maxlength="9" type="text" name="cep"
                                                    value="{{pessoa_juridica.nr_cep}}" class="form-control"
                                                    placeholder="Preencha o CEP">
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="fw-bold">Cidade</label><br>
                                                <select class="form-select form-control" name="cidade">
                                                    <option value="1" {{#if (eq pessoa_juridica.cd_cidade "1"
                                                        )}}selected{{/if}}>Curitiba</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-md-6">
                                            <div class="form-group">
                                                <label class="fw-bold">Bairro</label><br>
                                                <select class="form-select form-control" name="bairro">
                                                    <option value="1" {{#if (eq pessoa_juridica.cd_bairro "1"
                                                        )}}selected{{/if}}>Xaxim</option>
                                                </select>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="d-flex justify-content-end mt-3">
                                        <button type="submit" class="btn btn-success">Salvar</button>
                                    </div>
                                </form>

                            </div>
